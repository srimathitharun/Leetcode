class Solution {
    public ListNode middleNode(ListNode head) {
        ListNode curr = head;
        int count = 0;
        while (curr != null) {
            curr = curr.next;
            count++;
        }

        int mid = count / 2;

        curr = head;
        for (int i = 0; i < mid; i++) {
            curr = curr.next;
        }

        return curr;
    }
}
