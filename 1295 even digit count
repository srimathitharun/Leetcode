class Solution {
    public int findNumbers(int[] nums) {
        int evenCount = 0;

        for (int i = 0; i < nums.length; i++) {
            int rev = 0;
            int count = 0;
            int n = nums[i];

            while (n != 0) {
                rev = rev * 10 + n % 10;
                n = n / 10;
                count++;
            }

            if (count % 2 == 0) {
                evenCount++;
            }
        }

        return evenCount;
    }
}

